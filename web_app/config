server {
  listen  80;
  server_name reservetontrain;
  access_log /var/log/rtt.troll/nginx.access.log;
  error_log  /var/log/rtt.troll/nginx.error.log;
  root /home/rtt/;
  charset utf-8;
  index index.html index.htm;

  location / {
    rewrite ^ https://$host$request_uri? permanent;
  }

  location ~ ^/(scripts.*js|styles|images) {
    gzip_static on;
    expires 1y;
    add_header Cache-Control public;
    break;
  }
}

